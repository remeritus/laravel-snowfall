{
    "name": "remeritus/laravel-snowfall",
    "description": "A configurable snowfall effect for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Sihm",
            "email": "development@remerit.us",
            "homepage": "https://remerit.us",
            "role": "Developer"
        }
